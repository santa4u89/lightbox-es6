{
  "name": "gulp-dev-stack",
  "version": "1.0.0",
  "description": "Gulp dev stack project",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "arrowFunctions": true,
      "spread": true,
      "restParams": true,
      "templateStrings": true,
      "blockBindings": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "semi": 2,
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "keyword-spacing": 2,
      "one-var": [
        2,
        "never"
      ],
      "no-empty": 2,
      "space-in-parens": [
        2,
        "never"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "no-spaced-func": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 1
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^4.0.0-alpha.2",
    "browser-sync": "^2.10.0",
    "browserify": "^13.0.0",
    "copy-paste": "^1.1.4",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.5.2",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-css-globbing": "^0.1.8",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-nunjucks": "^2.1.1",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.0",
    "gulp-prettify": "^0.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-svgmin": "^1.2.2",
    "gulp-svgstore": "^6.0.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.7",
    "postcss-font-magician": "^1.4.0",
    "postcss-pxtorem": "^3.3.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^4.1.0"
  },
  "main": "gulpfile.babel.js",
  "dependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
